{
  "handlers": {
    "file": {
      "type": "pipe",
      "command": "/etc/sensu/extensions/event-file.rb",
      "timeout": 10
    },
    "tcp_socket": {
      "type": "tcp",
      "socket": {
        "host": "influxdb",
        "port": 6000
      }
    },
    "transport": {
      "type": "transport",
      "pipe": {
        "type": "direct",
        "name": "events"
      }
    },
    "default": {
      "type": "set",
      "handlers": [
        "file",
        "tcp_socket",
        "transport"
      ]
    },
    "influxdb": {
      "type": "udp",
      "mutator": "influxdb_line_protocol",
      "socket": {
        "host": "influxdb",
        "port": 8089
      }
    }
  }
}
